files:
  "/etc/nginx/conf.d/nginx_tweaks.conf":
    mode: "000644"
    owner: root
    group: users
    content: |
      proxy_buffering on;
      proxy_buffer_size 128k;
      proxy_buffers 4 256k;
      proxy_busy_buffers_size 256k;
      large_client_header_buffers 8 32k;
      client_max_body_size 300M;
